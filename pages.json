	// 分包预载配置
	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": ["pages/subPack"]
		},
		"pages/user/index": {
			"network": "all",
			"packages": ["pages/subPackUser"]
		}
	},

	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "演示模版",
		"navigationBarBackgroundColor": "#fdd000",
		"backgroundColor": "#ffffff"
	},
	"tabBar": {
		"color": "#898C9D",
		"selectedColor": "#fdd000",
		"borderStyle": "white",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/images/home.png",
			"selectedIconPath": "static/images/home-s.png",
			"text": "首页"
		}, {
			"pagePath": "pages/categoryList/categoryList",
